    "version": "0.2.0",
    "configurations": [

        {
            "name": "poc-frontend",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "--no-enable-impeller",
            ]
        },
        {
            "name": "poc-frontend (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
